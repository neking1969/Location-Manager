# Enneking Wealth - Environment Variables
# Copy this to .env and fill in your values

# Plaid API (https://dashboard.plaid.com)
# Sign up at plaid.com, get sandbox keys free, production requires approval
PLAID_CLIENT_ID=your_plaid_client_id
PLAID_SECRET=your_plaid_secret
PLAID_ENV=sandbox
# Options: sandbox (testing), development (limited live), production (full live)

# Finnhub Stock API (https://finnhub.io)
# Free tier: 60 calls/min, real-time US stock quotes
FINNHUB_API_KEY=your_finnhub_api_key

# Server
PORT=5002
NODE_ENV=development
CLIENT_URL=http://localhost:3000

# AWS (for deployment)
AWS_REGION=us-west-2
S3_BUCKET=enneking-wealth-frontend
API_DOMAIN=api.ennekingwealth.com
